<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"

       xsi:schemaLocation="
     http://www.springframework.org/schema/beans
     http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
     ">

    <bean class="net.bus.web.common.config.RString">
        <!-- 申请类型 -->
        <property name="APPLICATION_POOR" value="贫困建档申请"/>
        <property name="APPLICATION_GRANT" value="国家助学金申请"/>
        <property name="APPLICATION_MOTIVATIONAL" value="国家励志奖学金申请"/>
        <property name="AUDIT_COUNSELOR_TRUE" value="通过"/>
        <property name="AUDIT_COUNSELOR_FALSE" value="未通过"/>
        <property name="AUDIT_ADMIN_TRUE" value="通过"/>
        <property name="AUDIT_ADMIN_FALSE" value="未通过"/>
        <property name="AUDIT" value="未审核"/>

        <!-- File Upload -->
        <property name="FILE_UPLOAD_FAILED_EMPTY_FILE" value="上传文件为空"/>
        <property name="FILE_UPLOAD_FAILED_EXT" value="上传文件类型不正确"/>



    </bean>
</beans>